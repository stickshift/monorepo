{
  "name": "uv",
  "$schema": "{workspaceRoot}/node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nx/uv/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "maxWarnings": 0
      },
      "configurations": {
        "check": {
          "fix": false,
          "cache": true
        },
        "fix": {
          "fix": true,
          "cache": false
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/nx/uv/jest.config.ts"
      }
    }
  }
}
