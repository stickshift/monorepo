{
  "name": "@stickshift/monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "uv sync",
    "lint:check": "npx nx run-many -t lint -c check",
    "lint:fix": "npx nx run-many -t lint -c fix",
    "clean:ts": "find . -name node_modules -type d -prune -exec rm -rf {} +",
    "clean:python": "find . -name .venv -o -name __pycache__ -type d -prune -exec rm -rf {} +",
    "clean:nx": "find . -name .nx -type d -prune -exec rm -rf {} +",
    "clean": "npm run clean:ts && npm run clean:python && npm run clean:nx"
  },
  "dependencies": {},

  "devDependencies": {
    "@nx/workspace": "^20.3.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "eslint": "^9.18.0",
    "nx": "20.3.2",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.21.0"
  }
}
