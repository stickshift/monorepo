{
  "name": "@stickshift/monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "uv sync",
    "test": "npx nx run-many -t test",
    "format:check": "npx nx format:check --all",
    "format:fix": "npx nx format:write --all",
    "lint:check": "npx nx run-many -t lint -c check",
    "lint:fix": "npx nx run-many -t lint -c fix",
    "types:check": "npx nx run-many -t types",
    "check": "npm run lint:check && npm run types:check && npm run format:check",
    "fix": "npm run lint:fix && npm run format:fix",
    "clean:ts": "find . -name node_modules -type d -prune -exec rm -rf {} +",
    "clean:python": "find . -name .venv -o -name __pycache__ -type d -prune -exec rm -rf {} +",
    "clean:outputs": "find . -name dist -o -name build -type d -prune -exec rm -rf {} +",
    "clean:nx": "find . -name .nx -type d -prune -exec rm -rf {} +",
    "clean": "npm run clean:ts && npm run clean:python && npm run clean:outputs && npm run clean:nx"
  },
  "dependencies": {
    "@nx/devkit": "20.3.2",
    "command-exists": "^1.2.9",
    "cross-spawn": "^7.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@iarna/toml": "^2.2.5",
    "@nx/eslint": "20.3.2",
    "@nx/eslint-plugin": "20.3.2",
    "@nx/jest": "20.3.2",
    "@nx/js": "20.3.2",
    "@nx/plugin": "20.3.2",
    "@nx/vite": "20.3.2",
    "@nx/web": "20.3.2",
    "@nx/workspace": "20.3.2",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/command-exists": "^1.2.3",
    "@types/cross-spawn": "^6.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "~22.1.0",
    "nx": "20.3.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "~3.8.1",
    "vitest": "^1.3.1"
  },
  "nx": {
    "includedScripts": []
  }
}
