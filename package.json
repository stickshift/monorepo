{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  
  "scripts": {
    "postinstall": "uv sync",
    "clean:node": "find . -name node_modules -type d -prune -exec rm -rf {} +",
    "clean:uv": "find . -name .venv -type d -prune -exec rm -rf {} +",
    "clean:nx": "find . -name .nx -type d -prune -exec rm -rf {} +",
    "clean": "npm run clean:node && npm run clean:uv && npm run clean:nx"
  },

  "devDependencies": {
    "@nx/workspace": "^20.3.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "eslint": "^9.18.0",
    "nx": "20.3.2",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.21.0"
  }
}
