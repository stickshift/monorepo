{
    "name": "monorepo",
    "version": "0.1.0",
    "private": true,
    "packageManager": "^bun@1.1.43",
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "install:ts": "bun install",
        "install:py": "uv sync",
        "install:all": "bun run install:ts; bun run install:py",
        "dev": "turbo run dev",
        "clean:projects": "turbo run clean",
        "clean:root": "rm -rf node_modules .venv",
        "clean": "bun run clean:projects && bun run clean:root"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/bun": "^1.1.16",
        "@types/react": "^19.0.4",
        "concurrently": "^9.1.2",
        "turbo": "^2.3.3",
        "typescript": "^5.7.3"
    }
}